<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="768.0" prefWidth="1366.0" styleClass="mainBgnColor" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller.QuizResultController">
   <children>
      <Button fx:id="btnExit" layoutX="32.0" layoutY="26.0" mnemonicParsing="false" onAction="#cmdExit" prefHeight="44.0" prefWidth="180.0" style="-fx-background-color: #ffffff;" text="Exit Quiz">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <VBox alignment="CENTER" layoutX="532.0" layoutY="238.0" prefHeight="235.0" prefWidth="303.0" spacing="30.0">
         <children>
            <Label text="Result">
               <font>
                  <Font name="Segoe UI Bold" size="64.0" />
               </font>
            </Label>
            <Label fx:id="lblquizDiff" alignment="CENTER" contentDisplay="CENTER" text="Easy" textFill="#89ff8b">
               <font>
                  <Font name="Segoe UI Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="lblsomeWord" text="Better luck next time" textFill="WHITE">
               <font>
                  <Font name="Segoe UI Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="lblquizGrade" text="you got a ..." textFill="WHITE">
               <font>
                  <Font name="Segoe UI Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="lblquizScore" text="0 / 10" textFill="WHITE">
               <font>
                  <Font name="Segoe UI Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
